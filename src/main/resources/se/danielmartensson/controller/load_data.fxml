<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.danielmartensson.controller.Load_Data">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Data">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#Set_Delimiter_For_CSV" text="Set Delimiter For CSV" />
            <MenuItem mnemonicParsing="false" onAction="#Load_CSV_Train_Data" text="Load CSV Train Data" />
                  <MenuItem mnemonicParsing="false" onAction="#Load_CSV_Eval_Data" text="Load CSV Eval Data" />
          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="Stream">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#Select_Input_Columns" text="Select Input Columns" />
                  <MenuItem mnemonicParsing="false" onAction="#Select_Output_Columns" text="Select Outputs Columns" />
               </items>
            </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <columns>
                <TableColumn prefWidth="75.0" text="C1" />
                <TableColumn prefWidth="75.0" text="C2" />
              </columns>
            </TableView>
      </children>
    </AnchorPane>
  </children>
</VBox>
