<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.danielmartensson.controller.Front">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#New_Model" text="New Model" />
            <MenuItem mnemonicParsing="false" onAction="#Open_Model" text="Open Model" />
            <Menu fx:id="Recent_Model" mnemonicParsing="false" text="Open Recent Model" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="Save_Model_MenuItem" mnemonicParsing="false" onAction="#Save_Model" text="Save Model" />
            <MenuItem fx:id="Save_Model_As_MenuItem" mnemonicParsing="false" onAction="#Save_Model_As" text="Save Model Asâ€¦" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" onAction="#Quit_JNonlinearControl" text="Quit JNonlinearControl" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#Load_Data" text="Load CSV Data" />
            <MenuItem mnemonicParsing="false" onAction="#Network_Model_Configuration" text="Network Model Configuration" />
            <MenuItem mnemonicParsing="false" onAction="#Train_Model" text="Train Model" />
            <MenuItem mnemonicParsing="false" onAction="#Evaluate_Model" text="Evaluate Model" />
            <MenuItem mnemonicParsing="false" onAction="#Simulate_Model" text="Simulate Model">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
                  <MenuItem mnemonicParsing="false" onAction="#Stop_Process" text="Stop Process">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="D" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
          </items>
        </Menu>
            <Menu mnemonicParsing="false" text="Generation">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#Generate_C_Code" text="Generate C-code" />
              </items>
            </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#About_JNonlinearControl" text="About JNonlinearControl" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <LineChart fx:id="lineChart" layoutY="-2.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <xAxis>
                <CategoryAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
            </LineChart>
            <TextArea fx:id="textArea" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
