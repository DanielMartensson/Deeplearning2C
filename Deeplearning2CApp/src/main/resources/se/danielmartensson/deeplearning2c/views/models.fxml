<?xml version="1.0" encoding="UTF-8"?>

<?import com.gluonhq.charm.glisten.mvc.View?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<View fx:id="models" stylesheets="@models.css" xmlns="http://javafx.com/javafx/8.0.161-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.danielmartensson.deeplearning2c.views.ModelsPresenter">
   <center>
      <ScrollPane fitToHeight="true" fitToWidth="true" BorderPane.alignment="CENTER">
         <content>
            <TableView fx:id="tableView" onMouseClicked="#clickedOnTableView">
              <columns>
                <TableColumn fx:id="columnName" prefWidth="75.0" text="Name" />
              </columns>
            </TableView>
         </content>
      </ScrollPane>
   </center>
</View>
